
<h1> Sign Up</h1>

<form action="<%= users_url %>" method="POST">

    <label> Username:
        <input type="text" name="user[user_name]" value="<%= @user ? @user.user_name : nil %>">
    </label>
    <br>
    <label > Password:
        <input type="password" name="user[password]" value="">
    </label>
    <br>
    <input type="submit" value="Create Account">

</form>